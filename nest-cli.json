{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/frontend/src",
  "monorepo": true,
  "root": "apps/frontend",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/frontend/tsconfig.app.json"
  },
  "projects": {
    "frontend": {
      "type": "application",
      "root": "apps/frontend",
      "entryFile": "main",
      "sourceRoot": "apps/frontend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/frontend/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    }
  }
}